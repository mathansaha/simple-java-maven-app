pipeline 
{
agent {
  label 'Devserver'
}
 
stages {

    stage('build')
    {
        steps
        {
            sh "mvn clean package"
        }
        post {
        success {
        // One or more steps need to be included within each condition's block.
        archiveArtificats artifacts: '**/target/".war'
        }
}


    }
        stage('test ')
    {
        steps
        {
            sh 'echo test'
        }

    }
        stage('deploy')
    {
        steps
        {
            sh 'echo deploy'
        }

    }
}



}